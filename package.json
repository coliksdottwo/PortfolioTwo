{
  "name": "portfolio-two",
  "version": "1.0.0",
  "private": true,
  "description": "Production-ready portfolio using HTML, JS, and Tailwind CSS",
  "scripts": {
    "build:css": "postcss src/styles.css -o dist/styles.css --env production",
    "dev:css": "postcss src/styles.css -o dist/styles.css --watch",
    "build": "npm run build:css",
    "start": "serve -s -l 5173"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^11.0.1",
    "serve": "^14.2.0",
    "tailwindcss": "^3.4.1"
  }
}
